<?xml version="1.0"?>
<package format="2">
  <name>bitbots_hcm</name>
  <version>1.0.0</version>
  <description>The bitbots_hcm package provides a state machine which controlls the current state of the robot.
  It will perform falling and standing-up handling by its own. It will also decide which other nodes can write motor
  goal positions.</description>

  <maintainer email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

  <author email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</author>
  <author email="info@bit-bots.de">Hamburg Bit-Bots</author>

  <license>MIT</license>


  <buildtool_depend>catkin</buildtool_depend>

  <depend>rospy</depend>

  <exec_depend>std_msgs</exec_depend>
  <exec_depend>humanoid_league_msgs</exec_depend>
  <exec_depend>bitbots_bringup</exec_depend>
  <exec_depend>dynamic_reconfigure</exec_depend>
  <exec_depend>bitbots_msgs</exec_depend>
  <exec_depend>wolfgang_description</exec_depend>
  <exec_depend>wolfgang_animations</exec_depend>
  <exec_depend>wolfgang_moveit_config</exec_depend>
  <exec_depend>dynamic_stack_decider</exec_depend>
  <exec_depend>humanoid_league_speaker</exec_depend>
  <exec_depend>humanoid_base_footprint</exec_depend>

  <export>
    <bitbots_documentation>
      <language>python3</language>
      <status>starts</status>
    </bitbots_documentation>
  </export>
</package>

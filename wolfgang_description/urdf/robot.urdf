<gazebo reference="r_foot">
<material>Gazebo/FlatBlack</material>
<kp>1000000.0</kp>
<kd>100.0</kd>
<mu1>1.0</mu1>
<mu2>1.0</mu2>
<fdir1>1 0 0</fdir1>
<maxVel>1.0</maxVel>
<minDepth>0.00</minDepth>
</gazebo>
<gazebo reference="l_foot">
<material>Gazebo/FlatBlack</material>
<kp>1000000.0</kp>
<kd>100.0</kd>
<mu1>1.0</mu1>
<mu2>1.0</mu2>

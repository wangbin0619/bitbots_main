<?xml version="1.0"?>
<package format="2">
  <name>bitbots_nav2</name>
  <version>0.1.7</version>
  <description>The bitbots_nav2 package contains config files and launch scripts for nav2 which does planning
  and plan execution</description>

  <maintainer email="5guelden@informatik.uni-hamburg.de">Jasper GÃ¼ldenstein</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>


  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>rosidl_default_generators</buildtool_depend>
  <exec_depend>rosidl_default_runtime</exec_depend>

  <exec_depend>std_msgs</exec_depend>
  <exec_depend>message_runtime</exec_depend>
  <exec_depend>gazebo_msgs</exec_depend>

  <depend>humanoid_league_msgs</depend>
  <depend>rclpy</depend>
  <depend>nav2_bringup</depend>
  <depend>base_local_planner</depend>
  <depend>ros2_numpy</depend>
  <doc_depend>bitbots_docs</doc_depend>

  <export>
    <bitbots_documentation>
      <language>-</language>
      <status>tested_viz</status>
    </bitbots_documentation>
  </export>
</package>

<?xml version="1.0"?>
<package format="2">
  <name>bitbots_dynamixel_debug</name>
  <version>1.0.0</version>
  <description>Collection of python scripts based on DynamixelSDK to debug strange servo behaviour.</description>

  <maintainer email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>
  <author email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</author>
  <author email="info@bit-bots.de">Hamburg Bit-Bots</author>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>rospy</depend>

  <exec_depend>dynamixel_sdk</exec_depend>

  <export>
    <bitbots_documentation>
      <language>python</language>
      <status>tested_robot</status>
    </bitbots_documentation>
  </export>
</package>

<launch>
  <include file="$(find-pkg-share bitbots_utils)/launch/base.launch" />
  <node name="rviz" pkg="rviz" exec="rviz" output="screen" required="true"
    args="-d $(find-pkg-share wolfgang_description)/config/wolfgang.rviz" />
  <node name="motor_goals_viz_helper" pkg="bitbots_utils" exec="motor_goals_viz_helper.py"  args="--all" />

  <param from="$(find-pkg-share bitbots_dynup)/config/dynup_robot.yaml" command="load"/>
  <node name="dynup" pkg="bitbots_dynup" exec="DynupNode" output="screen" />
</launch>

<?xml version="1.0"?>
<package format="2">
  <name>bitbots_localization</name>
  <version>0.4.1</version>
  <description>The bitbots_localization package.</description>
  <maintainer email="2hartfil@informatik.uni-hamburg.de">Judith Hartfill</maintainer>
  <license>MIT</license>

  <!-- Use build_depend for packages you need at compile time: -->
  <build_depend>message_generation</build_depend>
  <!-- Use build_export_depend for packages you need in order to build against this package: -->
  <!--   <build_export_depend>message_generation</build_export_depend> -->
  <!-- Use exec_depend for packages you need at runtime: -->
  <exec_depend>message_runtime</exec_depend>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>geometry_msgs</depend>
  <depend>particle_filter</depend>
  <depend>roscpp</depend>
  <depend>std_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>humanoid_league_msgs</depend>
  <depend>dynamic_reconfigure</depend>
  <depend>rospy</depend>
  <depend>opencv</depend>
  <depend>roslib</depend>
  <exec_depend>map_server</exec_depend>

  <test_depend>bitbots_test</test_depend>

  <export>

  </export>
</package>

<?xml version="1.0"?>
<package format="2">
  <name>bitbots_localization</name>
  <version>0.0.2</version>
  <description>The bitbots_localization package.
    fake_pointcloud --> pointcloud2laserscan --> amcl
    Field models can be created with the map_generator script in bitbots_tools. </description>

  <maintainer email="2hartfil@informatik.uni-hamburg.de">Judith Hartfill</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>
  <author email="2hartfil@informatik.uni-hamburg.de">Judith Hartfill</author>
  <author email="info@bit-bots.de">Hamburg Bit-Bots</author>
  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>amcl</exec_depend>

  <depend>std_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>map_server</depend>
  <depend>robot_localization</depend>
  <depend>pointcloud_to_laserscan</depend>
  <depend>rospy</depend>
  <depend>humanoid_league_msgs</depend>
  <depend>geometry_msgs</depend>


  <export>
     <bitbots_documentation>
      <language>python</language>
      <status>unknown</status>
    </bitbots_documentation>
  </export>
</package>

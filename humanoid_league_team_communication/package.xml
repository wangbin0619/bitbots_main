<?xml version="1.0"?>
<package format="2">
  <name>humanoid_league_team_communication</name>
  <version>1.0.1</version>
  <description>The humanoid_league_team_communication package provides a ROS wrapper for the MiteCom standard communication
  protocol developed by the FUmanoids. </description>
  
  <maintainer email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</maintainer>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>

  <author email="bestmann@informatik.uni-hamburg.de">Marc Bestmann</author>
  <author email="5flemmin@informatik.uni-hamburg.de">Tanja Flemming</author>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>std_msgs</depend>
  <depend>humanoid_league_msgs</depend>
  <depend>roscpp</depend>
  <depend>geometry_msgs</depend>

  <export>
    <bitbots_documentation>
        <status>tested_viz</status>
      <language>c++</language>
    </bitbots_documentation>
  </export>
</package>
